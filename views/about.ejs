<% include partials/header %>

<h1>IT Department</h1>

<table id="table" class="table table-dark">  
    <thead>  
        <tr>  
            <th>Id</th>  
            <th>First name</th>  
            <th>Last name</th>  
            <th>Location</th>  
        </tr>  
    </thead>  
     <tbody>  
     <% result.forEach(function (user) { %>
        <tr>  
            <td> <a href="<%= 'users/' + user.id %>"</a><%= user.id %></a></td>  
            <td><%= user.firstname %></td>  
            <td><%= user.lastname %></td>
            <td><%= user.location %></td>

        </tr>                                   
     <% }) %>
     </tbody>
    </table>

<% include partials/footer %>


